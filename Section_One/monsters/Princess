function monsterrun(){
ponysay -b round -F celestiasmall "Thank you, hero, for rescuing me from this terrible place!"
echo "The Princess, freed from the magic-suppressing qualities of the cold iron cell, works her magic to open a portal to escape. A door opens up called 'portal' and you are free to step through it when you are ready. Once through, you can get back to your friends waiting outside." | fold -sw $(checkColumns) 
ln -s "$DUNGEON" portal
export SUCCESS=1
moneymoneymoney 30
}