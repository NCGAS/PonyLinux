function monsterrun(){
ponysay -b round -F owl "This is going to be a hoot."
echo "An owl is perched above you in an alcove in the stone wall. You thought it was a statue until it moves its head to regard you. You get the feeling that it will not hesitate to eat you if you died in here." | fold -sw $(checkColumns) 
export DUNGEON=$(pwd)
export HEALTH=100
}