function monsterrun(){
ponysay -b round -F owl "This is going to be a hoot."
echo "Dungeon goals: Explore the dungeon using ls, cd, chmod, and find. You need to figure out the path to the Princess, cd into the directory that she is in, then exit the dungeon. The dungeon is huge, so going through every directory exhaustively might not be wise!" | fold -sw $(checkColumns) 
export DUNGEON=$(pwd)
export HEALTH=100
}
